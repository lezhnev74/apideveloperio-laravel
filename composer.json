{
    "name": "lezhnev74/apideveloperio-laravel",
    "description": "Laravel package to track each http request/response and send them to apideveloper.io",
    "type": "library",
    "tags": [
        "http",
        "performance",
        "analytics",
        "logging",
        "request",
        "response"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Dmitriy Lezhnev",
            "email": "lezhnev.work@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "illuminate/support": "~5.4|~5.5|~5.6",
        "guzzlehttp/guzzle": "~6.0|~5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0|~6.0|~7.0",
        "orchestra/testbench": "^3.4|^3.5|^3.6"
    },
    "autoload": {
        "psr-4": {
            "HttpAnalyzer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HttpAnalyzerTest\\": "tests/"
        }
    }
}
